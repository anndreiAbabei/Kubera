{
  "AppSettings": {
    "DatabaseRetries": 3,
    "Autorisation": {
      "Authority": "https://localhost:44352/",
      "Audience": "Kubera.AppAPI",
      "ValidIssuers": [
        "https://localhost:44352/"
      ],
      "MapInboundClaims": false
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "IdentityServer": {
    "Clients": {
      "Kubera.App": {
        "Profile": "IdentityServerSPA"
      }
    }
  },
  "AllowedHosts": "*"
}
